{
  "expo": {
    "name": "forezy-mobile",
    "slug": "forezy-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/forezy-logo.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/icons/forezy-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/forezy-logo.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "userInterfaceStyle": "dark",
      "navigationBar": {
        "backgroundColor": "#000000"
      },
      "statusBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      }
    },
    "web": {
      "favicon": "./src/assets/icons/forezy-logo.png",
      "userInterfaceStyle": "dark",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
